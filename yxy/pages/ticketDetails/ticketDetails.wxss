/* 客服弹窗 */

.contact_bg {
  width: 100%;
  height: 100%;
  background-color: rgba(127, 127, 127, 0.6);
  position: fixed;
  z-index: 100;
  bottom: 0;
}

.contact_bg .la {
  width: 80%;
}

.detail {
  position: fixed;
  top: 20%;
  left: 50%;
  transform: translateX(-50%);
  z-index: 10;
  border-radius: 6rpx;
}

.cont_kefu {
  background-color: #fff;
  padding: 0px 0 80rpx;
}

.cont_kefu_p {
  color: #808080;
  font-size: 26rpx;
  text-align: center;
}

.cont_kefu_body {
  margin: 80rpx auto 0;
  width: 440rpx;
  padding-bottom: 40rpx;
  text-align: center;
}

.bor_b {
  border-bottom: 1rpx solid #fafafa;
}

.kefu_erweima {
  margin: 0 auto;
  text-align: center;
}

.cont {
  font-size: 30rpx;
  color: #333;
  padding: 40rpx 0;
}

.erweima {
  width: 200rpx;
  height: 200rpx;
  border: 2rpx solid #ebebeb;
  margin: 0 auto;
}

.img {
  height: 100%;
  max-width: 100%;
  vertical-align: middle;
}

.descr {
  text-align: center;
  font-size: 30rpx;
  color: #808080;
  margin-top: 10rpx;
}

.phone_number {
  text-align: center;
  font-size: 30rpx;
  color: #00a0e9;
  padding: 20rpx;
  border: 1rpx solid #00a0e9;
  border-radius: 40rpx;
  margin: 0 auto;
  width: 440rpx;
}

.close_but_img {
  width: 54rpx;
  height: 54rpx;
  display: block;
  border-radius: 50%;
  overflow: hidden;
  position: absolute;
  right: -18rpx;
  top: -23rpx;
}

.boda {
  position: relative;
  left: -24rpx;
}

.phone_number .iconTel {
  /* width: 0.42667rem; */
  display: inline-block;
  margin-right: 20rpx;
  vertical-align: 1px;
}

.phone_number .iconTel > image {
  height: 28rpx;
  width: 28rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
/*立即购买的弹窗 */

.verify_phone {
  width: 100%;
  height: 100%;
  background-color: rgba(127, 127, 127, 0.6);
